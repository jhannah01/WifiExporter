<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <ListPreference
        android:defaultValue="normal"
        android:entries="@array/pref_map_type_titles"
        android:entryValues="@array/pref_map_type_values"
        android:key="map_type"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_map_type" />
    
    <CheckBoxPreference
        android:defaultValue="true"
        android:key="use_my_location"
        android:title="@string/pref_title_use_my_location" />
</PreferenceScreen>